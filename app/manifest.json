{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*quoteStep1.jsp*",
        "https://*/*quoteStep1.jsp*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "scripts/customer/load.js"
      ],
      "css": [
        "styles/maps.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "scripts/customer/customer.js"
  ]
}
